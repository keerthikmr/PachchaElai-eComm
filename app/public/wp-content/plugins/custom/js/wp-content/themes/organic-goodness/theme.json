{
    "version": 2,
    "settings": {
        "appearanceTools": true,
        "custom": {
            "lineHeight": {
                "body": 1.5,
                "heading": 1.3
            }
        },
        "color": {
            "palette": [
                {
                    "slug": "color-1",
                    "color": "var(--global--body-color-1)",
                    "name": "Color 1"
                },
                {
                    "slug": "color-2",
                    "color": "var(--global--body-color-2)",
                    "name": "Color 2"
                },
                {
                    "slug": "color-3",
                    "color": "var(--global--body-color-3)",
                    "name": "Color 3"
                },
                {
                    "slug": "color-4",
                    "color": "var(--global--body-color-4)",
                    "name": "Color 4"
                },
                {
                    "slug": "color-5",
                    "color": "var(--global--body-color-5)",
                    "name": "Color 5"
                },
                {
                    "slug": "color-6",
                    "color": "var(--global--body-color-6)",
                    "name": "Color 6"
                }
            ]
        },
        "typography": {
            "customFontSize": true,
            "customFontFamily": true,
            "lineHeight": true,
            "fontStyle": true,
            "fontWeight": true,
            "dropCap": true,
            "fontFamilies": [
                {
                    "fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
                    "slug": "system-font",
                    "name": "System Font"
                },
                {
                    "fontFamily": "var(--global--font-primary)",
                    "slug": "primary-font",
                    "name": "Base Font Family"
                },
                {
                    "fontFamily": "var(--global--font-secondary)",
                    "slug": "secondary-font",
                    "name": "Headings Font Family"
                }
            ],
            "fontSizes": [
                {
                    "slug": "extra-small",
                    "size": "smaller",
                    "name": "Extra small"
                },
                {
                    "slug": "small",
                    "size": "small",
                    "name": "Small"
                },
                {
                    "slug": "normal",
                    "size": "var(--global--font-size-base)",
                    "name": "Normal"
                },
                {
                    "slug": "medium",
                    "size": "1.25rem",
                    "name": "Medium (H5)"
                },
                {
                    "slug": "large",
                    "size": "1.563rem",
                    "name": "Large (H4)"
                },
                {
                    "slug": "extra-large",
                    "size": "1.953rem",
                    "name": "Extra large (H3)"
                },
                {
                    "slug": "extra-extra-large",
                    "size": "2.441rem",
                    "name": "Extra extra large (H2)"
                },
                {
                    "slug": "huge",
                    "size": "3.052rem",
                    "name": "Huge (H1)"
                },
                {
                    "slug": "post-title",
                    "size": "4.111rem",
                    "name": "Post Title"
                },
                {
                    "slug": "page-title",
                    "size": "5.222rem",
                    "name": "Page Title"
                }
            ]
        },
        "spacing": {
            "padding": true,
            "margin": true,
            "blockGap": true,
            "units": [ "px", "em", "rem", "vh", "vw", "%" ]
        },
        "layout": {
            "contentSize": "775px",
            "wideSize": "65vw"
        }
    },
    "styles": {
        "spacing": {
            "blockGap": "1.55rem"
        },
        "color": {
            "background": "var(--wp--preset--color--color-1)",
            "text": "var(--wp--preset--color--color-2)"
        },
        "typography": {
            "fontFamily": "var(--global--font-primary)",
            "lineHeight": "var(--wp--custom--line-height--body)"
        },
        "elements": {
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--color-2)"
                },
                "typography": {
                    "fontStyle": "normal",
                    "fontWeight": "normal",
                    "fontFamily": "var(--global--font-primary)",
                    "lineHeight": "var(--wp--custom--line-height--body)"
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--huge)",
                    "fontWeight": "normal",
                    "fontFamily": "var(--global--font-headings)",
                    "lineHeight": "var(--wp--custom--line-height--heading)"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--extra-extra-large)",
                    "fontWeight": "normal",
                    "fontFamily": "var(--global--font-headings)",
                    "lineHeight": "var(--wp--custom--line-height--heading)"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--extra-large)",
                    "fontWeight": "normal",
                    "fontFamily": "var(--global--font-headings)",
                    "lineHeight": "var(--wp--custom--line-height--heading)"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--large)",
                    "fontWeight": "normal",
                    "fontFamily": "var(--global--font-headings)",
                    "lineHeight": "var(--wp--custom--line-height--heading)"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)",
                    "fontWeight": "normal",
                    "fontFamily": "var(--global--font-headings)",
                    "lineHeight": "var(--wp--custom--line-height--heading)"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--normal)",
                    "fontWeight": "normal",
                    "fontFamily": "var(--global--font-headings)",
                    "lineHeight": "var(--wp--custom--line-height--heading)"
                }
            }
        }
    }
}
